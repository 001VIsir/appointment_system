{
  "name": "appointment-system-frontend",
  "private": true,
  "version": "1.0.0",
  "description": "Appointment System Frontend - Vue 3 + TypeScript + Element Plus",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
